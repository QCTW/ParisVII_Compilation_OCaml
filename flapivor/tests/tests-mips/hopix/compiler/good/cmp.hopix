fun print_bool (b : bool) : unit =
  if b then print_string ("true") else print_string ("false")

val main =
  print_bool (3 < 3);
  print_string ("\n");
  print_bool (3 > 3);
  print_string ("\n");
  print_bool (3 = 3);
  print_string ("\n");
  print_bool (3 <= 3);
  print_string ("\n");
  print_bool (3 >= 3)
